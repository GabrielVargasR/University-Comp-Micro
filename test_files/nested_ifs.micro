-- Este programa lee tres números del usuario para determinar qué número imprimir
-- Además de demostrar el anidamiento de expresiones condicionales, evidencia la optimización de operaciones entre literales

begin
read(op1, op2, op3);
var1 := 7 + 8-9+2;
var2 := 2 + (3-4);
var3 := (var1+var2) -2;
var4 := (1|2|3);

nest := (op1|(op2|var2+234|(op3-var4|var3|132))|var1);
write(nest);
end